[
  {
    "type": "text",
    "label": "**Configs**",
    "margin": "10px 0 15px 0",
    "size": "20px"
  },
  {
    "type": "separator",
    "label": "Base Theme"
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.compact-marginless",
    "label": "Remove Margins From Compact Mode"
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.compact-marginless-hide-bookmarks",
    "label": "Hides the Bookmarks Bar in Marginless Compact Mode",
    "conditions": [
      {"if": {"property": "natsumi.theme.compact-marginless", "value": true}}
    ]
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.consistent-toolbar-button-size",
    "label": "Consistent Button Size"
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.enable-border-animation",
    "label": "Enables a Border Loading Animation for Tabs - May Be GPU Heavy"
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.force-blur",
    "label": "Force Background Blue Regardless of Whether a Blurred Element is Shown"
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.force-glass-dark-shadows",
    "label": "Forces Dark Shadow on Natsumi Glass"
  },
  {
    "type": "checkbox",
    "property": "natsumi.theme.opaque-icons",
    "label": "Makes Toolbar Icon Outlines Opaque"
  },
  {
    "type": "separator",
    "label": "URLbar"
  },
  {
    "type": "checkbox",
    "property": "natsumi.urlbar.disabled",
    "label": "Disable Natsumi URLbar"
  },
  {
    "type": "checkbox",
    "property": "natsumi.urlbar.disable-transparency",
    "label": "Disable URLbar Background Transparency",
    "conditions": [
      {"if": {"property": "natsumi.urlbar.disabled", "value": false}}
    ]
  },
    {
    "type": "checkbox",
    "property": "natsumi.urlbar.force-nowrap",
    "label": "Prevents Wrapping to Make Things More Compact Regardless of Window Width",
    "conditions": [
      {"if": {"property": "natsumi.urlbar.disabled", "value": false}}
    ]
  },
    {
    "type": "checkbox",
    "property": "natsumi.urlbar.no-compact",
    "label": "Disables some animations and Makes the Natsumi URLbar Lighter",
    "conditions": [
      {"if": {"property": "natsumi.urlbar.disabled", "value": false}}
    ]
  },
    {
    "type": "checkbox",
    "property": "natsumi.urlbar.light",
    "label": "Disables Comapact URLbar When Not Focused",
    "conditions": [
      {"if": {"property": "natsumi.urlbar.disabled", "value": false}}
    ]
  },
  {
    "type": "separator",
    "label": "Navbar"
  }
]
